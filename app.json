{
  "expo": {
    "name": "AI Study App",
    "slug": "ai-study-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourdomain.aistudyapp",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to capture images of notes for AI processing.",
        "NSPhotoLibraryUsageDescription": "This app accesses your photos to upload images of notes for AI processing."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["CAMERA", "READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE"],
      "package": "com.yourdomain.aistudyapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to upload images of notes for AI processing.",
          "cameraPermission": "The app uses the camera to capture images of notes for AI processing."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "7824da99-06da-4111-97c2-d197b53f2a38"
      },
      "FIREBASE_API_KEY": "your-firebase-api-key",
      "FIREBASE_AUTH_DOMAIN": "your-firebase-auth-domain",
      "FIREBASE_PROJECT_ID": "your-firebase-project-id",
      "FIREBASE_STORAGE_BUCKET": "your-firebase-storage-bucket",
      "FIREBASE_MESSAGING_SENDER_ID": "your-firebase-messaging-sender-id",
      "FIREBASE_APP_ID": "your-firebase-app-id",
      "GOOGLE_CLOUD_API_KEY": "your-google-cloud-api-key",
      "GOOGLE_CLOUD_PROJECT_ID": "your-google-cloud-project-id",
      "OPENAI_API_KEY": "your-openai-api-key",
      "HUGGINGFACE_API_KEY": "your-huggingface-api-key",
      "ONESIGNAL_APP_ID": "your-onesignal-app-id",
      "ONESIGNAL_SAFARI_WEB_ID": "your-onesignal-safari-web-id"
    }
  }
}

